<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\Rezerwacje" table="rezerwacje">
    <indexes>
      <index name="fk_rezerwacje_uzytkownicy_idx" columns="uzytkownicy_id_uzytkownika"/>
      <index name="fk_rezerwacje_seanse1_idx" columns="seanse_id_seansu"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_rezerwacje_UNIQUE" columns="id_rezerwacji"/>
    </unique-constraints>
    <id name="idRezerwacji" type="smallint" column="id_rezerwacji">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dataRezerwacji" type="datetime" column="data_rezerwacji" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP(6)</option>
      </options>
    </field>
    <field name="dataSeansu" type="date" column="data_seansu" nullable="false"/>
    <many-to-one field="uzytkownicyUzytkownika" target-entity="Uzytkownicy" fetch="LAZY">
      <join-columns>
        <join-column name="uzytkownicy_id_uzytkownika" referenced-column-name="id_uzytkownika"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="seanseSeansu" target-entity="Seanse" fetch="LAZY">
      <join-columns>
        <join-column name="seanse_id_seansu" referenced-column-name="id_seansu"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
