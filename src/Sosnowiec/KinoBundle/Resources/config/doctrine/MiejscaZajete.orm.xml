<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\MiejscaZajete" table="miejsca_zajete">
    <indexes>
      <index name="fk_miejsca_zajete_seanse1_idx" columns="seanse_id_seansu"/>
      <index name="fk_miejsca_zajete_miejsca1_idx" columns="miejsca_id_miejsca"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idmiejsca_zajete_UNIQUE" columns="idmiejsca_zajete"/>
    </unique-constraints>
    <id name="idmiejscaZajete" type="integer" column="idmiejsca_zajete">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="zajete" type="boolean" column="zajete" nullable="true"/>
    <many-to-one field="seanseSeansu" target-entity="Seanse" fetch="LAZY">
      <join-columns>
        <join-column name="seanse_id_seansu" referenced-column-name="id_seansu"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="miejscaMiejsca" target-entity="Miejsca" fetch="LAZY">
      <join-columns>
        <join-column name="miejsca_id_miejsca" referenced-column-name="id_miejsca"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
