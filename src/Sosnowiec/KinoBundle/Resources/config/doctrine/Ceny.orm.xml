<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\Ceny" table="ceny">
    <unique-constraints>
      <unique-constraint name="idceny_UNIQUE" columns="idceny"/>
    </unique-constraints>
    <id name="idceny" type="integer" column="idceny">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rodzaj" type="string" column="rodzaj" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cena" type="decimal" column="cena" precision="10" scale="0" nullable="false"/>
    <field name="cenaWeekend" type="decimal" column="cena_weekend" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
