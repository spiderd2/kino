<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\Bilety" table="bilety">
    <indexes>
      <index name="fk_bilety_rezerwacje1_idx" columns="rezerwacje_id_rezerwacji"/>
      <index name="fk_bilety_miejsca1_idx" columns="miejsca_id_miejsca"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_biletu_UNIQUE" columns="id_biletu"/>
    </unique-constraints>
    <id name="idBiletu" type="integer" column="id_biletu">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rodzaj" type="string" column="rodzaj" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cenyBiletowIdCenyBiletow" type="integer" column="ceny_biletow_id_ceny_biletow" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="cena" type="decimal" column="cena" precision="10" scale="0" nullable="false"/>
    <many-to-one field="rezerwacjeRezerwacji" target-entity="Rezerwacje" fetch="LAZY">
      <join-columns>
        <join-column name="rezerwacje_id_rezerwacji" referenced-column-name="id_rezerwacji"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="miejscaMiejsca" target-entity="Miejsca" fetch="LAZY">
      <join-columns>
        <join-column name="miejsca_id_miejsca" referenced-column-name="id_miejsca"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
