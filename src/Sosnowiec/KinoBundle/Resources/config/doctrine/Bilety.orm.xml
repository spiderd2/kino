<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\Bilety" table="bilety">
    <indexes>
      <index name="fk_bilety_rezerwacje1_idx" columns="rezerwacje_id_rezerwacji"/>
      <index name="fk_bilety_miejsca1_idx" columns="miejsca_id_miejsca"/>
      <index name="fk_bilety_ceny1_idx" columns="ceny_idceny"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_biletu_UNIQUE" columns="id_biletu"/>
    </unique-constraints>
    <id name="idBiletu" type="integer" column="id_biletu">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="rezerwacjeRezerwacji" target-entity="Rezerwacje" fetch="LAZY">
      <join-columns>
        <join-column name="rezerwacje_id_rezerwacji" referenced-column-name="id_rezerwacji"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="miejscaMiejsca" target-entity="Miejsca" fetch="LAZY">
      <join-columns>
        <join-column name="miejsca_id_miejsca" referenced-column-name="id_miejsca"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="cenyceny" target-entity="Ceny" fetch="LAZY">
      <join-columns>
        <join-column name="ceny_idceny" referenced-column-name="idceny"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
