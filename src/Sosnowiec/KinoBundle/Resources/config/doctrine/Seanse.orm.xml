<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\Seanse" table="seanse">
    <indexes>
      <index name="fk_seanse_filmy1_idx" columns="filmy_id_filmu"/>
      <index name="fk_seanse_sale_kinowe1_idx" columns="sale_kinowe_id_sale_kinowe"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idseanse_UNIQUE" columns="id_seansu"/>
    </unique-constraints>
    <id name="idSeansu" type="integer" column="id_seansu">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rozpoczecie" type="datetime" column="rozpoczecie" nullable="false"/>
    <field name="zakonczenie" type="datetime" column="zakonczenie" nullable="false"/>
    <many-to-one field="saleKinoweSaleKinowe" target-entity="SaleKinowe" fetch="LAZY">
      <join-columns>
        <join-column name="sale_kinowe_id_sale_kinowe" referenced-column-name="id_sale_kinowe"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="filmyFilmu" target-entity="Filmy" fetch="LAZY">
      <join-columns>
        <join-column name="filmy_id_filmu" referenced-column-name="id_filmu"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
