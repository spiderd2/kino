<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\Uzytkownicy" table="uzytkownicy">
    <unique-constraints>
      <unique-constraint name="id_uzytkownika_UNIQUE" columns="id_uzytkownika"/>
      <unique-constraint name="login_UNIQUE" columns="login"/>
      <unique-constraint name="email_UNIQUE" columns="email"/>
    </unique-constraints>
    <id name="idUzytkownika" type="smallint" column="id_uzytkownika">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="login" type="string" column="login" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="haslo" type="string" column="haslo" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="254" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imie" type="string" column="imie" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nazwisko" type="string" column="nazwisko" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefon" type="string" column="telefon" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
