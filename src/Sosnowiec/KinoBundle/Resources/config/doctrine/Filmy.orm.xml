<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sosnowiec\KinoBundle\Entity\Filmy" table="filmy">
    <unique-constraints>
      <unique-constraint name="idfilmy_UNIQUE" columns="id_filmu"/>
      <unique-constraint name="filmweb_id_UNIQUE" columns="filmweb_id"/>
    </unique-constraints>
    <id name="idFilmu" type="integer" column="id_filmu">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tytul" type="string" column="tytul" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="filmwebId" type="integer" column="filmweb_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
